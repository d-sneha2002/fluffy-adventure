def calculate_grade(marks):
  """Calculates the grade based on the given marks."""

  if marks > 90:
    return "A"
  elif marks >= 80:
    return "B"
  elif marks >= 60:
    return "C"
  else:
    return "D"

percentage = float(input("Enter percentage: "))
grade = calculate_grade(percentage)
print("Grade:", grade)
